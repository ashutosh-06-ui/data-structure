#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter number of students:" << endl;
    cin >> n;

    int rollNumbers[n];
    cout << "Enter roll numbers:" << endl;
    for (int i = 0; i < n; i++) {
        cin >> rollNumbers[i];
    }

    int searchRoll;
    cout << "Enter roll number to search:" << endl;
    cin >> searchRoll;

    bool found = false;
    for (int i = 0; i < n; i++) {
        if (rollNumbers[i] == searchRoll) {
            cout << "Roll number found at index position: " << i << endl;
            found = true;
            break;
        }
    }

    if (!found) {
        cout << "Roll number not found." << endl;
    }

    return 0;
}
